package day_100;

public class OperasiAritmatikaDay17 {
    public static void main(String[] args) {
        int a = 20;
        int b = 10;
        
        System.out.println("Hasil sisa Bagi = " + (a%b));
        System.out.println("Hasil kali = " + (a*b));
        System.out.println("Hasil dari a ditambah = " +(a++));
    }
    
}

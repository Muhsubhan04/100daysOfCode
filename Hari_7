package nama;

public class Main {
    
    public  static  void  main ( String [] args ) {
        String  nama , agama , alamat ;

        nama = "subhan" ;
        agama = "islam" ;
        alamat = "sendana" ;

        System.out.println( "======biodata==" );
        System.out.println( "Nama :" + nama );
        System.out.println( "Agama : " + agama );
        System.out.println( "Alamat :" + alamat );
        System.out.println( "=============================" ) ;


    }

}

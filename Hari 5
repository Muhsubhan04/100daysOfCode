package coding_100hari;


public class hari5 {
    public static void main(String[] args) {
        int panjang=12;
        int lebar=23;
        
        
        int rumus = panjang*lebar;
        int kel = 2*(panjang+lebar);
        System.out.println("luas    :"+rumus+"cm");
        System.out.println("Keliling:"+kel+"cm");
        
        
        
    }
}
